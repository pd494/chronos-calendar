    "lucide-react": "^0.562.0",
    "react": "^18.3.1",
    "react-colorful": "^5.6.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.69.0",
    "react-router-dom": "^7.11.0",
    "rrule": "^2.8.1",
    "sonner": "^2.0.7",
    "tauri-plugin-keyring-api": "^0.1.1",
    "zod": "^4.3.4",
    "zustand": "^5.0.9"
