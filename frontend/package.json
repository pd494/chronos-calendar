{
  "name": "chronos-calendar",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "tauri": "tauri",
    "dev:tauri": "tauri dev",
    "build:tauri": "tauri build",
    "build:tauri-dev": "tauri build --debug --config src-tauri/tauri.conf.dev.json",
    "install:tauri-dev": "npm run build:tauri-dev && node -e \"const fs=require('fs');const {execFileSync}=require('child_process');const c=JSON.parse(fs.readFileSync('src-tauri/tauri.conf.dev.json','utf8'));const name=(c.productName||'Chronos Calendar Dev')+'.app';execFileSync('cp',['-rf',`src-tauri/target/debug/bundle/macos/${name}`,'/Applications/'],{stdio:'inherit'});\""
  },
  "dependencies": {
